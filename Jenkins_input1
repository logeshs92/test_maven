pipeline {
  agent any
  stages {
stage('Choose environment') {
  userInput = ""
  userInput = input(id: 'userInput', message: 'Choose an environment', parameters: [[$class: 'ChoiceParameterDefinition', choices: "yes\no", name: 'Env']])
}
  stage('test') {
	steps {
		sh label: '', script: '''
		if [ userInput.Env -eq yes ];
		then 
		echo "yes"
		else
		echo "no"
		fi'''
  	  }
   	}
  }
}
